generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model contact_page {
  id        Int    @id @default(autoincrement())
  full_name String @db.VarChar(255)
  email     String @db.VarChar(255)
  messages  String @db.VarChar(255)
}

model experience_page {
  id             Int    @id @default(autoincrement())
  year           String
  title          String @db.VarChar(255)
  description    String @db.VarChar(255)
  location       String @db.VarChar(255)
  category       String @db.VarChar(255)
}

model portfolio_page {
  id                        Int     @id(map: "portfolio_pkey") @default(autoincrement())
  title                     String  @db.VarChar(255)
  description               String?
  earlier_image_url         String?
  recent_image_url          String?
  earlier_image_public_id   String?
  recent_image_public_id    String?
}